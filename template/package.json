{
  "name": "{{ name }}",
  "version": "1.0.0",
  "description": "{{ description }}",
  "author": "{{ author }}",
  "private": true,
  "scripts": {
    "dev": "webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
    "start": "npm run dev",
    "e2e": "node test/e2e/runner.js",
    "e2e:reporter": "nightwatch-html-reporter -d test/e2e/reports",
    "unit": "cross-env BABEL_ENV=test karma start test/unit/karma.conf.js",
    "unit:dev": "cross-env BABEL_ENV=test karma start test/unit/karma.conf.js --no-single-run",
    "test": "npm run unit && npm run e2e",
    "lint": "eslint --ext .js,.vue src test/unit",
    "build": "node build/build.js",
    "build:prod": "node build/build.js --prod"{{#commit_check}},
    "deploy:dev": "npm run build && mandy deploy dev",{{#if_in options "mandy"}}
    "deploy:prod": "{{#if_in options "qiniu"}}cross-env CDN_ENV=qiniu {{/if_in}}npm run build:prod && mandy deploy prod",{{/if_in}}
    "commitmsg": "npm run checkCommit",
    "checkCommit": "node scripts/check-commit-message.js"{{/commit_check}}
  },
  "dependencies": {
    "axios": "^0.18.0",
    "babel-polyfill": "^6.26.0",
    "eruda": "^1.4.0",{{#if_in options "sentry"}}
    "raven-js": "^3.24.0",{{/if_in}}
    "vue": "^2.5.2",
    "vue-class-component": "^6.2.0",
    "vue-property-decorator": "^6.0.0",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1",
    "vuex-class": "^0.3.0"
  },
  "devDependencies": {
    "@types/chai": "^4.1.3",
    "@types/karma": "^1.7.3",
    "@types/mocha": "^5.2.0",
    "@types/node": "^7.0.60",
    "@types/webpack-env": "^1.13.5",
    "autoprefixer": "^7.1.2",
    "babel-core": "^6.22.1",
    "babel-eslint": "^8.2.1",
    "babel-helper-vue-jsx-merge-props": "^2.0.3",
    "babel-jest": "^21.0.2",
    "babel-loader": "^7.1.1",
    "babel-plugin-istanbul": "^4.1.6",
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-plugin-transform-vue-jsx": "^3.5.0",
    "babel-preset-env": "^1.3.2",
    "babel-preset-stage-2": "^6.22.0",{{#if_in options "qiniu"}}
    "better-qiniu-webpack-plugin": "0.0.2",{{/if_in}}
    "chai": "^4.1.2",
    "chalk": "^2.0.1",
    "chromedriver": "^2.30.1",
    "copy-webpack-plugin": "^4.0.1",{{#if_in options "qiniu"}}
    "cross-env": "^5.1.4",{{/if_in}}
    "css-loader": "^0.28.0",
    "detect-port": "^1.2.2",
    "eslint": "^4.15.0",
    "eslint-config-airbnb-base": "^11.3.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-vue": "^4.0.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^1.1.4",
    "fork-ts-checker-webpack-plugin": "^0.4.1",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "html-webpack-plugin": "^2.30.1"{{#commit_check}},
    "husky": "^0.14.3"{{/commit_check}},
    "istanbul-instrumenter-loader": "^3.0.1",
    "karma": "^2.0.2",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "^1.0.1",
    "karma-coverage": "^1.1.1",
    "karma-coverage-istanbul-reporter": "^2.0.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^3.0.0"{{#commit_check}},
    "lint-staged": "^7.0.0"{{/commit_check}},{{#if_in options "mandy"}}
    "mandy": "^0.1.5",{{/if_in}}
    "mocha": "^5.1.1",
    "nightwatch": "^0.9.21",
    "nightwatch-html-reporter": "^2.0.5",
    "node-notifier": "^5.1.2",
    "optimize-css-assets-webpack-plugin": "^3.2.0",
    "ora": "^1.2.0",
    "portfinder": "^1.0.13",
    "postcss-import": "^11.0.0",
    "postcss-loader": "^2.0.8",
    "postcss-url": "^7.2.1",
    "rimraf": "^2.6.0",
    "selenium-server": "^3.12.0",
    "semver": "^5.3.0",
    "shelljs": "^0.7.6",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "uglifyjs-webpack-plugin": "^1.1.1",
    "ts-loader": "^3.2.0",
    "tslint": "^5.9.1",
    "tslint-config-standard": "^7.0.0",
    "tslint-loader": "^3.6.0",
    "typescript": "^2.7.2",
    "url-loader": "^0.5.8",
    "vue-jest": "^1.0.2",
    "vue-loader": "^13.3.0",
    "vue-style-loader": "^3.0.1",
    "vue-template-compiler": "^2.5.2"{{#commit_check}},
    "validate-commit-msg": "^2.14.0"{{/commit_check}},
    "webpack": "^3.6.0",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-dev-middleware": "^1.10.0",
    "webpack-dev-server": "^2.9.1",
    "webpack-hot-middleware": "^2.16.1",
    "webpack-merge": "^4.1.0"
  },
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]{{#commit_check}},
  "config": {
    "validate-commit-msg": {
      "types": [
        "feat",
        "fix",
        "docs",
        "style",
        "refactor",
        "test",
        "ci",
        "chore",
        "perf",
        "revert",
        "temp",
        "update",
        "change"
      ]
    }
  }{{/commit_check}}
}
